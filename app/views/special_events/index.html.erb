<h1>Your Special Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Type</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @special_event.each do |special_event| %>
      <tr>
        <td><%= special_event.name %></td>
        <td><%= special_event.description %></td>
        <td><%= special_event.type %></td>
        <td><%= link_to 'Show', special_event %></td>
        <td><%= link_to 'Edit', edit_special_event_path(special_event) %></td>
        <td><%= link_to 'Destroy', special_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

